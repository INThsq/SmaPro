/* pages/Details/Details.wxss */
.swipercontent {
  position: relative;
}
.container{
  padding-bottom: 50px;
}

.swiper{
  width: 100%;
  height:744.5rpx;
}
.nl{
  font-size:18px!important;
}
#two{
  height: 100vh;
  background: #F43736;
}
.two{
  font-size: 29.17rpx!important;
}
.swiper image {
  display: block;
  width: 100%;
  height: 100%;
}
.dots,.dotes{
  position: absolute;
  bottom:30rpx;
  display: flex;
  justify-content: center;
}

.dotes{
  right:30rpx!important;
}
.tabs{
  margin-top: 0;
}
.nums{
width:35px;
height:18px;
background:rgba(237,237,237,1);
border-radius:100px;
font-size: 12px;
color: #666;
text-align: center;
}
/* 特权 */
.privilege image{
  width: 14px;
  height: 18px;
  vertical-align: middle;
  margin-right: 10rpx;
  margin-top: -1px;
}
.privilege{
  padding: 26rpx 0 16rpx 30rpx;
  background:linear-gradient(90deg,rgba(255,93,91,1) 0%,rgba(255,150,71,1) 100%);
  color: white;
  display: flex;
}
.pri-left{
  flex:0 0 70%;
}
.pri-left .tequan{
  font-size: 33.3rpx;font-weight: bold;
}
.present{
  font-size: 25rpx;
}
.pri-right{
  flex: 0 0 100px;
  font-size: 29.17rpx;
  text-align: center;
  margin:  auto auto;
  border-left:2px solid #FFA074;
}
/* 价格 */
.price,.title{
  background: white;
  display: flex;
  padding: 20rpx 30rpx;
}
.title{
  display: inline-block!important;
  padding-top: 0!important;
  font-size: 33.3rpx;
  line-height: 24px;
  padding-bottom: 30rpx!important;
}
.circle{
  font-size: 10px;
  border:1px solid #FF0000;
  border-radius: 4px;
  color: #FF0000;
  padding: 0 3px;
}
.yang{
  font-size: 25rpx!important;
}
.one{
  font-size:33.3rpx;
  color: #FF2742;
  margin-right:30rpx;
}
.two{
  font-size:29.17rpx;
  color: #999;
  margin-top:10rpx;
  text-decoration:line-through;
}
.top-right .two{
  color: #666;
}
.top-right .ys{
  font-size: 25rpx;
  color: #333;
}
.ys{
  font-size:29.17rpx;
  color:#666;
  margin-left: 32rpx;
  margin-top:8rpx;
}
.yes{
  margin-left: 0!important;
}
/* //规格 */
.choose{
  margin-top: 20rpx;
}
.choose-one,.choose-two,.evaluate{
  padding: 28rpx 28rpx 28rpx 30rpx;
  color: #333;
  background: white;
  display: flex;
  justify-content: space-between;
  margin:  auto auto;
  border-bottom:.5px solid #F3F3F3;
}
.iconfont{
  color: #999!important;
  line-height: 1.4;
}
.choose-two text{
  font-size: 29.17rpx;  
  color: #666;
}
.o,.evaluate{
  font-size: 33.3rpx;
}
.choose-two,.evaluate{
  border-bottom: none!important;
}
.evaluate{
  margin-top: 20rpx;
}
.choose-two .iconfont {
  line-height: 2!important;
  font-size: 32rpx!important;
}
.zp{
  background:url('http://oss.myzy.com.cn/wechat/images/icon_gouxuan.png') no-repeat left;
  background-size: 12px 12px;
  padding-left: 22px;
}
.zp:nth-child(2){
  margin: 0 30rpx;
}
/* 店铺 */
.store{
  display: flex;
  background: white;
  padding: 30rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}
.tit{
  font-size:33.3rpx;
  font-weight: bold;
  color:#333;
  line-height: 24px;
}
.small{
  font-size: 25rpx;
  color: #666;
  margin-top: 4px;
}
.store-details text{
  display: block;
}
.store-btn button::after{
  border: none;
}
.store-btn button{
  width:116rpx;
  height: 56rpx;
  background:#F43736;
border-radius:4px;
font-size: 25rpx;
color: white;
text-align: center;
padding: 0!important;
line-height:56rpx;
}
.store-btn{
float:flex;
  position: absolute;
  right:34rpx;
  margin-top: 12px;
}
.store view image{
  width: 45px;
  height: 45px;
  border-radius: 4px;
}
.store-image{
  width: 45px;
  height: 45px;
  margin-right:26rpx;
  
}
.store-details{
  width: 60%;
}
/* 店铺推荐 */
.recommend{
  background: white;
  margin-top: 20rpx;
}
.rec-title{
  padding: 30rpx;
  font-size: 33.3rpx;
  color: #333;
}
.rec-conts{
  padding: 0  30rpx  30rpx;
}
.rec-conts{
  display: flex;
  flex: 1;
}
.rec-detail:nth-of-type(2){
  margin: 0 6px;
}
.rec-detail .nl{
  font-size: 12px!important;
  font-weight: 400!important;
}
.rec-detail .two{
  font-size:8px!important;
  font-weight: 400!important;
  float: right;
  margin-top: 14rpx;
  text-decoration:inherit!important;
}
.rec-detail .one{
margin-right: 0!important;
margin-left:-10px;
position: relative;
}
.rec-detail{
  width: 33%;
  text-align: center;
}
.titles{
  font-size: 25rpx;
  color:#333;
display: -webkit-box;
overflow : hidden;
text-overflow: ellipsis;
display: -webkit-box;
-webkit-line-clamp:1;
-webkit-box-orient: vertical;

}
.wrapper{
  width: 100%;
  height:110rpx;
  font-size:36rpx;
  font-weight:bold;
  padding-top:30rpx;
  line-height: 110rpx;
  background:white;
  letter-spacing: 2rpx;
  font-family:SourceHanSansSC-Medium;
  color:#333;
  position: fixed;
  text-align: center;
  justify-content:center;
  display: flex;
  top: 0;
  z-index: 9999;
  }
  .back{
  position: absolute;
  left:15px;
  margin-top:8rpx;
}
.back image{
  width:40rpx;
  height:28rpx;
}
.rec-conts image{
  width:212rpx;
  height: 212rpx;
}
/* 上拉显示更多 */
.up-all{
  margin-top: 20rpx;
  background: white;
  font-size: 29.17rpx;
  color:#666;
  text-align: center;
  padding: 30rpx 0 40rpx;
}
.up-all image{
  width: 12px;
  height: 10px;
  margin-right: 12rpx;
}

/* 底部 */
.footer{
  display: flex;
  position: fixed;
  bottom: 0;
  background: white;
  width: 100%;
  border-top:1px solid #EDEDED;
  
}
.footer .f-l{
  padding:16rpx 0 12rpx;
  box-sizing: border-box;
  height:104.17rpx;

}

.footer .f-l .shop,.service{
  padding: 0 50rpx;
}
.service text,.shop text{
  margin-top: -4rpx;
}

.footer image{
  width: 19px;
  height: 18px;
}
.footer>view{
  display: flex;
  text-align: center;
}
.sx{
    width: 1px!important;
    background: #EDEDED;
    height: 28px;
    margin-top: 5px;
}

.footer>view text{
  display: block;
  font-size: 25rpx;
  color: #666;
}
.footer button::after{
  border:none;
}
.footer button{
 position: absolute;
  right: 0;
width:160px;
height:104.17rpx;
background:#F43736;
border-radius: 0; 
font-size: 33.3rpx;
line-height:104.17rpx;
color: white; 
justify-content: right;
}
/* //弹出框 */
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.6;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}
/*对话框 */
.commodity_attr_box {
  padding: 30rpx;
  box-sizing: border-box;
  height:454px;
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  padding-top: 20rpx;
}
.chosoe-top{
  display: flex;
  height:85px;
  border-bottom: 1px solid #F3F3F3;
  padding-bottom: 20rpx;
}
.choose-top{
 position: absolute;
 right:30rpx;
}
.chosoe-top .top-left image{
  width:200rpx;
  height:200rpx;
  border-radius: 4rpx;
  display: inline-block;
  margin-top: 4rpx;
}
.commodity_attr_box .colors{
 margin-top: 26rpx;
  color: #333;
  font-size: 29.17rpx;
}
.chosoe-top .top-left{
  position: relative;
  top:-50rpx;
  z-index: 99999;
}
.color{
  color: white!important;
  background:#F43736!important;
}
.shul{
  color: #333;
  font-size: 29.17rpx;
  padding: 32rpx 0 64rpx;
}
.dis{display:inline-block;}
.dis view{
  display: inline-block;
  padding:14rpx 18rpx;
  background: #F4F4F4;
  border-radius: 4px;
  font-size: 25rpx;
  margin-right:24rpx;
  margin-top: 24rpx;
 
}
.two{
  display:block;
}
.ys{
  display: block;
}
.top-right .two{
  text-decoration: inherit;
}
.top-right text{
  line-height: 40rpx;
}
.top-right{
  padding-top:6rpx;
}
.top-left{
  margin-right: 40rpx;
  background:url('http://oss.myzy.com.cn/wechat/images/icon_guige_bwijing.png')no-repeat center;
  background-size: 100% 100%;
  width: 216rpx;
  height: 216rpx;
  overflow: hidden;
  padding:4rpx;
  box-sizing: border-box;
  text-align: center;
}
.choose-top image{
  width: 14px;
  height: 14px;
}
.commodity_attr_box scoll-view{
  padding-right: 10rpx;
}
/*主容器*/
.stepper {
	width:115px;
	height:25px;
	/*给主容器设一个边框*/
	float: right;
}
::-webkit-scrollbar{

width: 0;

height: 0;

color: transparent;

}
.nos{
  margin-right: 2px;
}
.no{
  margin-left: 2px;
}
.dis{
  padding-bottom: 28rpx;
  border-bottom: 1px solid #F3F3F3;
  width: 100%;
}
.scroll-views{
  height:560rpx;
  overflow: hidden;
}
.btn::after{
border:none;

}
.btn{
  width: 100%;
  height: 50px;
line-height: 50px;
background:#F43736;
color: white;
font-size: 33.3rpx;
border-radius: 0;
position: absolute;
bottom: 0;
left: 0;
}
/*加号和减号*/
.stepper text {
	width:32px;
	line-height:25px;
	text-align: center;
	float: left;
}
.shul .left{
width: 50%;
float: left;
line-height: 30px;
  border-radius: 2px;


}
 
/*数值*/
.stepper input {
	width: 46px;
	height:25px;
	float: left;
	margin: 0 auto;
	text-align: center;
	font-size: 12px;
  background: #F3F3F3;
  border-radius: 2px;
  
	/*给中间的input设置左右边框即可*/
}
 
/*普通样式*/
.stepper .normal{
	color: #979797;
  background: #F3F3F3;
  border-radius: 2px;
  
}
 
/*禁用样式*/
.stepper .disabled{
  width: 32px;
  height: 25px;
  color: #979797;
  background:#E2E2E2;
  border-radius: 2px;
  }
   .back{
  position: fixed;
  left:15px;
  top:60rpx;
}
.back image{
