/*首页跑马灯效果*/
@keyframes around {
  from {
   margin-left:50%;
  }
  to {
   /* var接受传入的变量 */
   margin-left: var(--marqueeWidth--);
  }
 }
 .marquee_container image{
    width:30rpx;
  height: 26rpx;
  margin-right:10rpx;
  z-index: 999;
  display: flex;
 }
 .imggs{
   position: absolute;
