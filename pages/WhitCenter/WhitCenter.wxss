 }
.marquee_container{
  position: relative;
  background-color: #fafafa;
  height:38px;
  line-height: 38px;
  width: 100%;
  margin-top:0rpx;
  padding-left: 100rpx;
  overflow: hidden;
}
.right{
  display: flex;
  position: absolute;
  width: 30rpx;
  background: white;
  height: 38px;
  right: 0;
  top: 0;
}
.marquee_container:hover{
  /* 不起作用 */
  animation-play-state: paused;
}
.marquee_text{
  color:#333;
  position: absolute;
  top: 0;
  font-size: 25rpx;
  display: inline-block;
  white-space: nowrap;
  animation-name: around;
  animation-duration:20s;  /*过渡时间*/
  animation-iteration-count: infinite;
  animation-timing-function:linear;
}
