/* pages/CardSucc/CardSucc.wxss */
.container{
  margin-top:144rpx;
  text-align: center;
}
.container image{
  width: 128rpx;
  height: 128rpx;
  margin:120rpx 0 64rpx;
}
.container text{
  display: inline-block;
  width: 100%;
}
.one{
  color: #333;
  font-size: 18px;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.two{
  color: #666;
  width: 100%;
  font-size: 29.17rpx;
}

.tr{
  width: 380rpx;
  height: 40px;
  line-height: 40px;
  box-shadow:0px 2px 6px 0px rgba(255,230,223,1);
  border-radius:4px;
  border:1px solid rgba(153,153,153,1);
  color: #666;
  font-size: 33.3rpx;
  position: absolute;
  bottom: 180rpx;
  left: 50%;
  margin-left:-190rpx;
}