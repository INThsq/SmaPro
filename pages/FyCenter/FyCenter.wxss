/** 模态 **/
.modal{
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    right: 0rpx;
    bottom: 0rpx;
    width: 100%;
    height: 100%; 
    z-index:99999;
  }
  
  .modal-mask{
    position: absolute;
    width: 100%;
    height: 100%;
    z-index: 97;
    background-color: rgba(0,0,0,0.6);
  }
  
  
  .modal-layer-sm{
     width: 60%; 
     transform : translate3d(-50%,-50%,0);
     left : 50%;  
  } 
  
  .modal-layer-md{
     width:544rpx; 
     transform : translate3d(-50%,-50%,0);
     left : 50%;    
  } 
  
  .modal-layer-full{
     width: 100%; 
     left: 0;  
  } 
  .modal-body{
    font-size:33.3rpx;
    color: #999;
  text-align: left;

  }
  .modal-body cover-view{
    font-size:25rpx;
    text-align: left;
  }
  .modal-header cover-view:first-child{
    font-size:33.3rpx;
  }
  .modal-layer{

    position: absolute;
    background: white;
    top: 50%;    
    display: flex;
    flex-direction: column;
    z-index: 98;   
    box-shadow: 0 4rpx 14rpx rgba(0,0,0,.4); 
  }
  
  .modal-header{
    background: #fff;
    color:  #333;
    padding:36rpx 48rpx 18rpx;
    font-size: 36rpx;
    text-align: left;
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;  
 }

  .modal-body{
    flex:  1;  
    padding:0 48rpx;
    background: #ffffff; 
    margin-bottom:50rpx; 
  color: #999;
  font-size: 33.3rpx;
  }

  

.modal-footer{
    background: #ffffff;
    flex-direction: row;
    display: flex;
    align-items: center;
    height: 84rpx;
    line-height: 84rpx;
    width: 100%;  
    justify-content: center;
    font-size: 29.17rpx;
    border-bottom-left-radius: 10rpx;
    border-bottom-right-radius: 10rpx;  
}
.modal-body cover-view{
  font-size: 29.17rpx;
}
.modal-close{
    color: #fff;
    font-size: 48rpx;
    position: absolute;
    right: 40rpx;
    top: 0;
    z-index: 98;
}


