<!-- 引用组件的页面模版 -->
<view>
  <my-component>
    <!-- 这部分内容将被放置在组件 <slot> 的位置上 -->
     <view>收益报表</view> 
  </my-component>
</view>


<!-- 提现 -->
<view class='withdraw'>
  <view class='withdraw_left'>
      <text class='mon_title'>可提现金额(元)</text>
      <text class='money'>{{callback.thaw_money}}</text>
  </view>
  <view class='withdraw_right'>
  
    <view class='btnes' bindtap='Deposit'>提现</view> 
      
  </view>
</view>

<!-- 全部收益 -->
<view class='all'>
<view  class="earn">
<view class='xian'></view>
<view class="myEarn">全部收益</view>
</view>
<view class='all_earn'>
    <view class='all_earn_item'>
      <navigator>
        <text class='m_text'>总收益(元)
        </text>
          <image src='http://oss.myzy.com.cn/wechat/images/icon_yiwen.png'></image>
        

        <text class='money'>{{callback.all_money.money}}</text>
        
      </navigator>
    </view>
    <view class='all_earn_item'>
      <navigator>
        <text class='m_text'>累计提现收益(元)</text>
        <text class='money'>{{callback.all_cash.money}}</text>
        
      </navigator>
    </view>
</view>
</view>

<!-- 全部订单 -->

<view class='all'>
  <view  class="earn">
  <view class='xian'></view>
  <view class="myEarn">全部订单</view>
  </view>
</view>

<!-- 选项卡 -->
<view class='tab'>
<view class="swiper-tab">
    <view class="swiper-tab-item {{currentTab==0?'active':''}}" data-id='1' data-current="0" bindtap="clickTab">今日</view>
    <view class="swiper-tab-item {{currentTab==1?'active':''}}"  data-id='2' data-current="1" bindtap="clickTab">近日</view>
    <view class="swiper-tab-item {{currentTab==2?'active':''}}" data-id='7' data-current="2" bindtap="clickTab">近一周</view>
     <view class="swiper-tab-item {{currentTab==3?'active':''}}" data-id='30' data-current="3" bindtap="clickTab">近一个月</view>
</view>
</view>

<view class='tab_cont'>
    <view class='conts'>
    <block wx:for="{{callback.total_list}}" wx:key="index">
        <view class='num'>
            <view class='zs'>
             {{item[0]}}
            </view>
            <view class='number'>
              {{item[1]}}
            </view>
        </view>
        </block>
    </view>
</view>


<!-- 慕态框 -->
<view class="commodity_screen"  wx:if="{{showModalStatus}}"></view>
<!--弹出框  -->
<view animation="{{animationData}}"  class="commodity_attr_box tx-box" wx:if="{{showModalStatus}}">
  <view class='titles'>
    <text class='title_left' bindtap='hideModal'>取消</text>
    <text class='title_center'>选择提现方式</text>
  </view>
  <block wx:for="{{callback.application}}" wx:key="{{index}}">
  <view class='item' bindtap='Choose'  data-jump='{{item.jump}}'>
      <image src='{{item.image}}' class='way'></image>
      <view class='item_right'>
        <text class='choose_one' decode="{{true}}">{{item.name}}</text>
        <!-- <text class='choose_two' decode="{{true}}">(暂未开通)</text> -->
      </view>
    </view>
  </block>
</view>

 <!-- load -->
  <view class="load {{isShow ? 'show' : ''}}" >
        <image class="bth_image2"  animation="{{animationData}}"   src='http://oss.myzy.com.cn/wechat/images/icon_jiazaishuaxin.png'></image>正在加载…
        </view>


