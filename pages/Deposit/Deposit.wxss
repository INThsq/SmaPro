/* pages/Deposit/Deposit.wxss */
.Deposit-cont{
  padding: 0 30rpx;
  margin-top: 154rpx;
  display: flex;
}
.Deposit-cont .Deposit-title{
  display: flex;
  width: 100%;
  flex: 1;
  background: white;
  height: 140rpx;
  padding: 50rpx;
  box-sizing: border-box;
  font-size: 32rpx;
  color: #333;
  box-shadow:0px 0px 4px 0px rgba(246,246,246,1);
  border-radius:6px; 
  letter-spacing: 1rpx; 
}
.pa{
  position: absolute;
line-height: 80rpx;
 font-size: 56rpx;
 

}

.Deposit-title image{
  width:76rpx;
  height: 64rpx;
  margin-right: 24rpx;
}

.Deposit-money{
  padding: 0 30rpx 10rpx 30rpx;
}
.Deposit-mon{
  background: white;
   box-shadow:0px 0px 4px 0px rgba(246,246,246,1);
  border-radius:6px; 
  margin-top: 24rpx;
  padding:30rpx 70rpx;
  box-sizing: border-box;
}
.tx-jine{
font-size: 32rpx;
color: #333;
letter-spacing: 1rpx;
}
.Deposit-mon input{
  border-bottom: .5px #f3f3f3 solid;
 height: 80rpx;
 margin-top: 24rpx;
 padding-left:56rpx;
 font-size: 56rpx;
 color: #333;
}
.fh{
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 999;
}
.about,.abouts{
  font-size: 26rpx;
  color: #666;
  letter-spacing: 1rpx;
}
.abouts{
  color: #FF0B00;
}
.all{
  line-height:1.6;
}
.Deposit-money button{
  margin-top: 24rpx;
  border: none;
  color: white;
}
.btns{
  background:#F43736;
  border-radius:4px;
  opacity:0.6;
  line-height: 42px;
height: 42px;
}
.btn{
  background:#F43736;
border-radius:4px;
line-height: 42px;
height: 42px;
}
button::after{
  border:none;
}
.about{
  font-size: 26rpx;
