<!--pages/Coindeposit/Coindeposit.wxml-->
<!-- 引用组件的页面模版 -->
<view class="wrapper">
  <view class='back' bindtap='back'>
    <image src='http://oss.myzy.com.cn/wechat/images/goback.png'></image>
  </view>
  <view class="tetxs" >充值成功</view>

</view>



<!-- 内容 -->
<view class='coin-cont'>
  <view class='coin-left'>
  <image src='http://oss.myzy.com.cn/wechat/images/icon_daozhangchenggong.png'></image>
  </view>
  <view class='coin-right'>
    <view class='fq'>
    发起提现申请
    </view>
    <view class='chuli'>
    <text>银行处理中</text>
    <text class='time'>预计2018-02-01 15:58到账</text>
    <text class='success'>到账成功</text>
    </view>
  </view>
</view>


<view class='coin-conts'>
  <view class='contes'>
  <text>提现金额</text>
  <text>￥{{txmoney}}</text>
  </view>
  <view class='contes'>
  <text>手续费</text>
  <text>￥{{service}}</text>
  </view>
</view>