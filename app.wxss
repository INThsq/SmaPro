/**app.wxss**/

page{
    color:#333;
    margin:0;
    background: #fafafa;
    font-family:"SourceHanSansSC";
}

/*global.css*/
.mainBg{
    background: #b4282d;
}
.mainColor{
    color: #b4282d;
}
.mainBorder{
    border: 1px solid #b4282d;
}
.baseMarginTop{
    margin-top: 30rpx;
}


/*热门标签 span*/
.hot{
    font-size: 22rpx;
    display: inline-block;
    text-align: center;
    padding: 2rpx 4rpx;
    color: #fff;
    margin-right: 4rpx;
    border-radius: 4rpx;
}
/*标签颜色*/
.hot1{
    background: #e5686d;
}
.hot2{
    background: #f48f18;
}

/*公共searchBar*/
#search-box{
    display: flex;
    padding:20rpx 4%;
}
#search-box .iconNav{
    flex:0 0 10%;
    line-height:60rpx;
    text-align:center;
}
#search-box .iconNav .iconfont{
    font-size:40rpx;
}
#search-box .search{
    flex:0 0 80%;
    height:60rpx;
    font-size:28rpx;
    line-height:60rpx;
    text-align:center;
    background: #fff;
    border-radius:10rpx;
    
}
.tetxs{
 width: 50%;
}


